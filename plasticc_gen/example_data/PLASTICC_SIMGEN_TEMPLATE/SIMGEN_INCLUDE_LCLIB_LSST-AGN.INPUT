GENMODEL:  LCLIB  $PLASTICC_MODELS/LCLIB_LSST-AGN.TEXT 1+2+3+4

# Rel rate = 5th order polynominal in cos(b)
DNDB: COSBPOLY 1  0 0 0 0 0        # AGN are isotropic
